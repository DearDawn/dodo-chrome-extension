{
  "manifest_version": 3,
  "name": "Hello Dodo",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    // "default_popup": "index.html",
    "default_icon": "rainbow.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "content_scripts": [
    {
      "css": [
        "index.7abb323d.css"
      ],
      "js": [
        "index.e9725252.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}