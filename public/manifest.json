{
  "manifest_version": 3,
  "name": "Dodo Sticky",
  "description": "A simple local sticky",
  "version": "0.0.3",
  "action": {
    "default_icon": "rainbow.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "content_scripts": [
    {
      "css": [
        "index.7abb323d.css"
      ],
      "js": [
        "index.e9725252.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}